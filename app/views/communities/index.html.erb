<div class="container">
  <div class="profile">
      <h1>Time Line</h1>
      <% @communities.each do |community| %>
        <%= community.content %>
      <% end %>
      
      <%= form_with model: @community, local: true do |f| %>
        <%= f.text_field :content %>
      <%= f.submit "Create" %>
      <% end %>
  </div>
</div>